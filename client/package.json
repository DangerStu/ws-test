{
	"name": "ws-test",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"lint": "vue-cli-service lint",
		"test": "yarn lint && yarn test:unit",
		"test:unit": "jest --watch",
		"test:unit:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --no-cache --runInBand"
	},
	"dependencies": {
		"@babel/core": "^7.11.6",
		"@vue/test-utils": "^1.1.0",
		"axios": "^0.20.0",
		"babel-core": "7.0.0-bridge.0",
		"babel-jest": "^26.3.0",
		"bootstrap": "^4.5.2",
		"bootstrap-vue": "^2.17.3",
		"core-js": "^3.6.5",
		"jest": "^26.4.2",
		"node-sass": "^4.14.1",
		"sass-loader": "^10.0.2",
		"vue": "^2.6.11",
		"vue-jest": "^3.0.7"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "~4.5.0",
		"@vue/cli-plugin-eslint": "~4.5.0",
		"@vue/cli-service": "~4.5.0",
		"babel-eslint": "^10.1.0",
		"eslint": "^6.7.2",
		"eslint-plugin-vue": "^6.2.2",
		"vue-template-compiler": "^2.6.11"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true,
			"jest": true
		},
		"extends": [
			"plugin:vue/essential",
			"eslint:recommended"
		],
		"parserOptions": {
			"parser": "babel-eslint"
		},
		"rules": {}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not dead"
	],
	"jest": {
		"transform": {
			"^.+\\.js$": "babel-jest",
			"^.+\\.vue$": "vue-jest"
		},
		"moduleFileExtensions": [
			"js",
			"json",
			"jsx",
			"ts",
			"tsx",
			"node",
			"vue"
		]
	}
}
